  "description": "Deobfuscate Javascript with emphasis on reconstructing strings",
  "main": "index.js",
  "type": "module",
  "bin": {
    "restringer": "./src/restringer.js"
  },
  "directories": {
    "prepare": "husky install",
    "test": "tests"
  },
  "dependencies": {
    "flast": "^2.0.2",
