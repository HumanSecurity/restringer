  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PerimeterX/Restringer/issues"
  },
  "homepage": "https://github.com/PerimeterX/Restringer#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.8",
    "@babel/plugin-syntax-import-assertions": "^7.25.7",
    "eslint": "^9.12.0",
    "husky": "^9.1.6"
  }
